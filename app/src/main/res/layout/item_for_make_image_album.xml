<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/imageForMakeAlbum"
    android:layout_height="@dimen/imageForMakeAlbum"
    style="@style/cardView_btn"
    tools:ignore="MissingConstraints"
    app:cardCornerRadius="5dp"
    android:layout_marginTop="5sp"
    android:layout_marginStart="5sp"
    >

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/AppCompatImageViewForMakeAlbum"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="0dp"
        android:contentDescription="@string/image_view_item_transform_content_description"
        tools:src="@tools:sample/avatars"
        style="@style/imageView"
        android:scaleType="fitXY"
        />
</androidx.cardview.widget.CardView>